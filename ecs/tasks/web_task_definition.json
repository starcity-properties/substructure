[
    {
        "name": "${name}",
        "image": "${image}",
        "cpu": ${cpu},
        "memory": ${memory},
        "portMappings": [
            {
                "containerPort": ${container_port},
                "hostPort": ${host_port},
                "protocol": "${protocol}"
            }
        ],
        "networkMode": "awsvpc",
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "${log_group}",
                "awslogs-region": "${aws_region}",
                "awslogs-stream-prefix": "${prefix}"
            }
        },
        "environment": []
    }
]
