[
    {
        "name": "${name}",
        "image": "${image}",
        "cpu": ${cpu},
        "memory": ${memory},
        "portMappings": [
            {
                "containerPort": ${container_port},
                "hostPort": ${host_port},
                "protocol": "${protocol}"
            }
        ],
        "networkMode": "awsvpc",
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "${log_group}",
                "awslogs-region": "${aws_region}",
                "awslogs-stream-prefix": "${prefix}"
            }
        },
        "environment": [
            {"name": "TIPE_ORG_SECRET",
             "value": "${tipe_org_secret}"
            },{
                "name": "TIPE_API_KEY",
                "value": "${tipe_api_key}"
            },{
                "name": "SLACK_CLIENT_ID",
                "value": "${slack_client_id}"
            },{
                "name": "SLACK_CLIENT_SECRET",
                "value": "${slack_client_secret}"
            },{
                "name": "SLACK_WEBHOOK",
                "value": "${slack_webhook}"
            },{
                "name": "SLACK_TOKEN",
                "value": "${slack_token}"
            },{
                "name": "DATOMIC_URI",
                "value": "${datomic_uri}"}
        ]
    }
]
